{
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.codeActionsOnSave": {
		"source.addMissingImports": "always",
		"source.fixAll": "always",
		"source.fixAll.eslint": "always",
		"source.organizeImports": "always"
	},
	"material-icon-theme.folders.associations": {
		".ai": "robot",
		"architect": "godot",
		"builder": "godot",
		"craftsman": "godot",
		"file": "meta",
		"http": "api",
		"prompts": "robot",
		"domain": "cobol",
		"system": "stack"
	},
	"material-icon-theme.files.associations": {
		"*.blueprint.md": "apiblueprint",
		"*.instructions.md": "twine",
		"*.tracker.md": "verified",
		"*.plan.md": "architecture",
		"*.prompt.md": "pascal",
		"*.template.md": "textlint",
		"*.adapter.ts": "adonis",
		"*.builder.ts": "unocss",
		"*.command.ts": "console",
		"*.component.ts": "html",
		"*.decorator.ts": "renovate",
		"*.dto.ts": "raml",
		"*.facade.ts": "mjml",
		"*.factory.ts": "fortran",
		"*.functions.ts": "url",
		"*.gateway.ts": "ifanr-cloud",
		"*.interface.ts": "raml",
		"*.mapper.ts": "purescript",
		"*.module.ts": "less",
		"*.operations.ts": "url",
		"*.singleton.ts": "stitches",
		"*.repository.ts": "database",
		"*.service.ts": "slint",
		"*.type.ts": "raml",
		"*.utils.ts": "tilt",
		"*.validator.ts": "velite"
	},
  "workbench.colorCustomizations": {
    "activityBar.activeBorder": "#007700",
    "commandCenter.activeBorder": "#007700",
    "commandCenter.inactiveBorder": "#007700",
    "editorCursor.foreground": "#007700",
    "editorError.foreground": "#00aaff",
    "editorWarning.foreground": "#00ffff",
    "focusBorder": "#007700",
    "inlineChat.border": "#007700",
    "panel.border": "#007700",
    "sideBar.border": "#007700",
    "statusBar.border": "#007700",
    "tab.activeBorder": "#007700",
    "tab.activeBorderTop": "#007700",
    "terminalCursor.foreground": "#007700",
    "tree.indentGuidesStroke": "#007700",
    "widget.border": "#007700"
  },
}
